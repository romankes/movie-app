{
  "name": "small-app-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint '**/*.{ts,tsx}'",
    "generate-module": "node ./scripts/create-new-module.js",
    "init-project": "node ./scripts/init-project.js",
    "generate-types": "cross-var openapi-typescript $npm_config_url --output ./modules/core/types/api-types.ts",
    "eas:build:ios:simulator": "dotenv -e .env -- eas build --profile development-ios-simulator --platform ios --local",
    "eas:build:ios:staging": "dotenv -e .env -- eas build --profile staging --platform ios",
    "eas:build:ios:production": "APP_ENV=production dotenv -e .env -- eas build --profile production --platform ios",
    "eas:build:android:staging": "dotenv -e .env -- eas build --profile staging --platform android",
    "eas:build:android:production": "APP_ENV=production dotenv -e .env -- eas build --profile production --platform android",
    "eas:submit:ios:staging": "dotenv -e .env -- eas submit --platform ios --profile staging",
    "eas:submit:ios:production": "APP_ENV=production dotenv -e .env -- eas submit --platform ios --profile production",
    "eas:submit:android:staging": "dotenv -e .env -- eas submit --platform android --profile staging",
    "eas:submit:andoird:production": "APP_ENV=production dotenv -e .env -- eas submit --platform android --profile production",
    "prebuild:production": "APP_ENV=production npx expo prebuild --clean",
    "prebuild:clean:android": "npx expo prebuild --clean -p android",
    "prebuild:clean:ios": "npx expo prebuild --clean -p ios",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@dev-plugins/react-query": "^0.0.7",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@shopify/flash-list": "^1.7.3",
    "@tanstack/query-async-storage-persister": "^5.59.13",
    "@tanstack/react-query": "^5.59.13",
    "@tanstack/react-query-persist-client": "^5.59.13",
    "axios": "^1.7.7",
    "expo": "^52.0.37",
    "expo-constants": "~17.0.7",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.1",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.24.2",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@cspell/eslint-plugin": "^8.14.4",
    "@react-native/metro-config": "^0.75.4",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
